nomor = input("Masukkan Angka Nomor NPM anda ")

genap = 0
ganjil = 0

for no in nomor:
    if int(no) % 2 == 0:
     genap += 1
    else:
     ganjil += 1

print("Jumlah angka bilangan genap:", genap)
print("Jumlah angka bilangan ganjil:", ganjil)

baris = int(input("Masukkan Tinggi yang diinginkan = "))

for i in range(1, baris + 1):
  for j in range(1, i + 1):
    print(i*j,end=" ")
  print()
